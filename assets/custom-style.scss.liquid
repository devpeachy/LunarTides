/* ========================== Product Banner ========================== */

.product-banner-section {  
    padding: 56px 0 97px;

    .product-container {
        max-width: 1200px;
        width: 100%;
        margin: 0 auto;
        overflow: hidden;

        .product-content-wrapper {
            display: flex;
            border: 4px solid #000000;

            .product-images-wrapper {
                display: flex;
                width: 63%;

                .product-image-wrapper {
                    width: 64%;

                    .product-image {
                        width: 100%;
                        height: 100%;
                        object-fit: cover;
                    }
                }

                .announcement-wrapper {
                    max-width: 64px;
                    transform: translateX(-468px);

                    .marquee-announcement-bar {
                        max-height: 64px;
                        width: 1000px;
                        overflow: hidden;
                        transform: rotate(90deg);
                    
                        &.download {
                        max-height: initial;
                        }
                    
                        .js-marquee {
                            display: flex;
                            align-items: center;
                            border-right: 0;
                            border-left: 0;
                            padding: 24px 0;
                            white-space: nowrap;
                        }
    
                        .announcement-bar__item {
                            position: relative;
                            font-family: Montserrat, sans-serif;
                            font-weight: 700;
                            font-size: 24px;
                            line-height: 16px;
                            margin-right: 16px;
                            transition: opacity .2s linear;
                    
                        &:hover {
                            opacity: 0.8;
                        }
                        }
                    }
                }
                
                .decoration-image-wrapper {
                    width: 190px;
                    .decoration-image-desk {
                        width: 100%;
                        height: 100%;
                        object-fit: cover;
                        display: inline-block;
                        border-right: 1px solid #000000;
                    }
                    .decoration-image-mob {
                        display: none;
                    }
                }
            }

            .product-title-wrapper {
                padding: 10px 0;
                width: 44%;
                display: flex;
                justify-content: center;
                flex-direction: column;
                align-items: center;
                line-height: normal;
                position: relative;
                z-index: 1;

                .prodact-title-background {
                    position: absolute;
                    top: 0;
                    left: 0;
                    height: 100%;
                    width: 100%;
                    object-fit: cover;
                    z-index: -1;
                }

                .product-banner-title {
                    margin-bottom: 40px;
                    font-weight: 800;
                    font-size: 80px;
                    text-transform: uppercase;
                    max-width: 400px;
                    text-align: center;
                    z-index: 1;
                }
            }
        }
    }
}

@media (max-width: 1024px){
    .product-banner-section {  
        .product-container {
            .product-content-wrapper {
                .product-title-wrapper {
                    .product-banner-title {
