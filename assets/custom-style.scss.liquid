.navbar-dropdown.navbar-dropdown-redesigned {
  max-width: 100vw;
  min-width: 200px;
  width: 100vw;
  left: -25px;
  padding: 0;
  top: 52px;
  box-shadow: none;

  @media (min-width: 1300px) {
    left: -80px;
  }

  @media (min-width: 1440px) {
    max-width: 1440px;
    width: 1440px;
  }

  &:before {
    content: '';
    position: absolute;
    height: 30px;
    width: 100%;
    left: 0;
    top: -29px;
  }

  &:after {
    content: '';
    position: absolute;
    top: 0;
    left: -50%;
    height: 100%;
    width: 10000%;
    z-index: 5;
    background-color: #FFFFFF;
    box-shadow: 0 8px 8px #0a0a0a1a;
  }

  .navbar-dropdown-redesigned__wrapper {
    position: relative;
    z-index: 10;
    display: flex;
    justify-content: space-between;
    max-width: 1440px;
    width: 100%;
    margin: 0 auto;
    padding: 24px 25px;

    @media (min-width: 1300px) {
      padding: 24px 80px;
    }

    .navbar-items {
      display: flex;
      flex-direction: column;
      flex-wrap: wrap;
      max-width: 380px;
      width: 100%;
      height: 200px;

      @media (min-width: 1100px) {
        max-width: 444px;
      }

      @media (min-width: 1200px) {
        max-width: 500px;
      }

      @media (min-width: 1350px) {
        max-width: 548px;
      }

      .navbar-item {
        padding: 0;
        margin: 0 0 8px !important;
        font-family: 'Montserrat', sans-serif;
        font-style: normal;
        font-weight: 700;
        font-size: 13px;
        line-height: 24px;
        color: #000000;
        transition: opacity .2s linear;

        @media (min-width: 1100px) {
          font-size: 14px;
        }

        @media (min-width: 1200px) {
          font-size: 16px;
        }

        &:hover {
          opacity: 0.6;
        }
      }
    }

    .navbar-blocks {
      max-width: 733px;
      width: 100%;
      display: grid;
      grid-template-columns: repeat(2, 1fr);
      grid-column-gap: 13px;

      @media (min-width: 1100px) {
        grid-column-gap: 23px;
      }

      .navbar-block {
        .navbar-block__img {
          margin: 0 0 12px;
          position: relative;
          padding-bottom: 58%;
          border: 1px solid #000000;

          a {
            display: block;
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;

            img {
              width: 100%;
              height: 100%;
              object-fit: cover;
              max-height: none;
            }
          }

          .badge {
            position: absolute;
            top: 8px;
            left: 8px;
            height: fit-content;
            width: fit-content;
            border-radius: 0;
            padding: 2px 4px;
            background-color: #532AB1;
            border: 1px solid #FFFFFF;

            font-family: 'Montserrat', sans-serif;
            font-style: normal;
            font-weight: 700;
            font-size: 12px;
            line-height: 14px;
            text-transform: uppercase;
            color: #FFFFFF;

            @media (min-width: 1100px) {
              padding: 4px 8px;
              top: 16px;
              left: 16px;
              font-size: 14px;
              line-height: 16px;
            }
          }
        }

        .navbar-block__content {
          display: flex;
          justify-content: space-between;

          .title {
            display: block;
            max-width: 200px;
            width: 100%;
            margin: 0 10px 0 0;

            font-family: 'Montserrat', sans-serif;
            font-style: normal;
            font-weight: 700;
            font-size: 11px;
            line-height: 19px;
            text-transform: uppercase;
            color: #000000;

            @media (min-width: 1100px) {
              font-size: 14px;
              line-height: 22px;
            }

            @media (min-width: 1200px) {
              font-size: 16px;
              line-height: 24px;
            }
          }

          .btn {
            flex-shrink: 0;
            height: fit-content;
            display: flex;
            align-items: center;
            width: fit-content;

            font-family: 'Montserrat', sans-serif;
            font-style: normal;
            font-weight: 700;
            font-size: 11px;
            line-height: 19px;
            text-transform: uppercase;
            color: #532AB1;
            transition: opacity .2s linear;

            @media (min-width: 1100px) {
              font-size: 14px;
              line-height: 22px;
            }

            @media (min-width: 1200px) {
              font-size: 16px;
              line-height: 24px;
            }

            &:hover {
              opacity: 0.6;
            }

            .arrow {
              display: none;
              margin: 0 0 0 10px;

              @media (min-width: 1100px) {
                display: block;
              }
            }
          }
        }
      }
    }
  }
