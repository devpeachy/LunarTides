/* === Product card === */
.product-thumbnail-redesign-wrapper {

  .product-image__wrapper {

    .thumbnail-overlay__container {
      justify-content: flex-end;
      background-color: transparent;

      .quick-shop__buttons {

        span.button {
          width: 100%;
          background: #FFFFFF;
          font-weight: 700;
          font-size: 12px;
          line-height: 24px;
          text-transform: uppercase;
          color: #000000;
          border-radius: 0;
          padding: 12px 12px;
          transition: all .2s linear;

          @media (min-width: 1366px) {
            font-size: 16px;
          }

          &:hover {
            background: #000000;
            color: #FFFFFF;
          }
        }
      }
    }

    .as-wishlist-btn {
      top: 9px;
      right: 9px;

      @media (min-width: 1023px) {
        top: 18px;
        right: 18px;
      }

      .swym-button.swym-add-to-wishlist-view-product {
        width: 24px;
        height: 24px;

        @media (min-width: 1023px) {
          width: 32px;
          height: 34px;
        }

        &::after {
          color: #FFFFFF !important;
          font-size: 24px;

          @media (min-width: 1023px) {
            font-size: 32px;
          }
        }
      }
    }

    .product-image__badges {
      max-width: 120px;
      position: absolute;
      display: flex;
      gap: 5px;
      flex-wrap: wrap;
      top: 12px;
      left: 8px;
      right: 8px;
      z-index: 25;

      @media (min-width: 1200px) {
        top: 16px;
        left: 16px;
        right: 16px;
      }

      .badge-item {
        padding: 4px 8px;
        font-weight: 700;
        font-size: 10px;
        line-height: 14px;
        text-transform: uppercase;

        @media (min-width: 1200px) {
          font-size: 14px;
          line-height: 16px;
        }

        &.badge-best-seller {
          color: #FFFFFF;
          background: #532AB1;
          border: 1px solid #FFFFFF;
        }
  
        &.badge-new {
          color: #000000;
          background: #FFFFFF;
          border: 1px solid #000000;
        }
      }
    }

    .image__container {

      .image-element__wrap {

        img {
          width: 100%;
          height: 100%;
          object-fit: cover;
        }
      }
    }

    .sticker-holder {
      position: absolute;
      z-index: 25;
      width: 40px;
      height: 40px;
      top: -8px;
      left: -8px;

      @media (min-width: 1200px) {
        width: 56px;
        height: 56px;
        top: -16px;
        left: -16px;
      }


      .sticker-holder__content {
        width: 100%;
        max-width: 100%;

        .thumbnail-sticker {
          padding: 0;
        }

        .sale-sticker {
          width: 100%;
          height: 100%;
          display: flex;
          align-content: center;
          align-items: center;
          background: #FFFFFF;
          border: 1px solid #000000;
          border-radius: 32px;

          .sticker-text {
            font-weight: 700;
            font-size: 10px;
            line-height: 14px;
            text-transform: uppercase;
            color: #000000;

            @media (min-width: 1200px) {
              font-size: 14px;
              line-height: 16px;
            }
          }
        }
      }
    }
  }

  .thumbnail__caption {
    text-align: left;

    .product-thumbnail {
      padding-top: 8px;

      @media (min-width: 1023px) {
        padding-top: 16px;
      }

      .product-thumbnail__title {
        padding: 0;
        font-weight: 700;
        font-size: 16px;
        line-height: 24px;
        text-transform: uppercase;
        color: #000000;

        @media (min-width: 767px) {
          min-height: auto;
        }
      }

      .product-thumbnail__price {
        text-align: left;

        small {
          font-weight: 400;
          font-size: 16px;
          line-height: 24px;
          text-transform: lowercase;
          color: #8F8F8F;
          margin-right: 8px;
        }

        span {
          font-weight: 700;
          font-size: 16px;
          line-height: 24px;
          text-transform: uppercase;
          color: #000000;
        }
      }

      .product-thumbnail__was-price {
        span {
          font-weight: 400;
          font-size: 16px;
          line-height: 24px;
          text-decoration-line: line-through;
          text-transform: lowercase;
          color: #8F8F8F;

        }
      }
    }
  }
}

/* === End Product card === */

