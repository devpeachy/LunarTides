/* ========================== Insta Feed ========================== */

.insta-feed-redesign {
  padding: 64px 0;

  @media (min-width: 1023px) {
    padding: 112px 0;
  }

  #insta-feed {
    margin: 0;

    h2 {
      display: none;
    }

    .instafeed-container {
      margin: -2px -2px 0 0;   
      border: 2px solid #000000;

      @media (min-width: 1023px) {
        margin: -4px -4px 0 0;   
        border: 4px solid #000000;
      }
    }
  }

  .insta-feed-redesign__header {
    max-width: 1440px;
    width: 100%;
    margin: 0 auto;
    padding: 0 16px;

    @media (min-width: 767px) {
      padding: 0 20px;
      display: flex;
      align-items: flex-end;
      justify-content: space-between;
      margin-bottom: 30px;
    }

    @media (min-width: 1023px) {
      padding: 0 40px;
      margin-bottom: 40px;
    }

    @media (min-width: 1366px) {
      padding: 0 80px;
    }


    .insta-feed-redesign__title {
      font-weight: 800;
      font-size: 24px;
      line-height: 32px;
      text-align: center;
      text-transform: uppercase;
      margin-bottom: 24px;

      @media (min-width: 767px) {
        margin-bottom: 0;
        text-align: left;
        font-size: 28px;
        max-width: 640px;
      }

      @media (min-width: 1023px) {
        font-weight: 700;
        font-size: 38px;
        line-height: 49px;
      }

      @media (min-width: 1366px) {
        font-size: 48px;
        line-height: 59px;
      } 
    }

    .insta-feed-redesign__subscribe {
      text-align: center;
      margin-bottom: 24px;

      @media (min-width: 767px) {
        margin-bottom: 0;
        text-align: right;
      }

      .subscribe-text {
        font-weight: 400;
        font-size: 16px;
        line-height: 24px;
        margin-bottom: 8px;
      }

      .insta {
        font-weight: 800;
        font-size: 24px;
        line-height: 32px;
        text-transform: uppercase;

        @media (min-width: 767px) {
          font-weight: 700;
        }
      }
    }
  }
}

/* ========================== END Insta Feed  ========================== */

// BUTTON 
.btn {
    position: relative;
    display: inline-block;
    width: auto;
    height: auto;
    transform: translateX(8px);
    transition: all .8s linear;

    .link {
        position: relative;
        padding: 12px 20px;
        text-align: center;
        background-color: #fff;
        font-size: 16px;
        font-weight: 700;
        color: #000;
        z-index: 10;
        display: inline-block;
    }
    
    .link-boxshadow {
        position: absolute;
        top: 8px;
        left: -8px;
        width: 100%;
        height: 100%;
        background-color: #7eeb28;
        z-index: 0;
    }
}
// BUTTON END


