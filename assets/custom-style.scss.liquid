/* ========================== Marquee announcement bar ========================== */
#marquee-announcement-bar {
  transition: all 1.1s;

  .marquee-announcement-bar {
    max-height: 0;
    overflow: hidden;

    &.download {
      max-height: initial;
    }

    .js-marquee {
      display: flex;
      align-items: center;
      border-right: 0;
      border-left: 0;
      padding: 8px 0;
      white-space: nowrap;
    }
  }

  .announcement-bar__item {
    position: relative;
    font-family: 'Druk Wide', sans-serif;
    font-weight: 700;
    font-size: 14px;
    line-height: 24px;
    margin-right: 76px;
    transition: opacity .2s linear;

    @media (min-width: 1024px) {
      margin-right: 56px;
    }

    &:hover {
      opacity: 0.8;
    }

    .decor-arrow {
      position: absolute;
      top: 50%;
      right: -47px;
      transform: translate(0, -43%);

      @media (min-width: 1024px) {
        right: -36px;
      }
    }
  }
}
/* ========================== Marquee announcement bar End ========================== */

/* ========================== Header Redesigned ========================== */
#header-redesigned {
  position: relative;
  height: auto;
  z-index: 5;

  &.sticky {
    position: fixed;
    top: 0;
    width: 100%;
  }

  &.active,
  &.with-bg {
    .header-redesigned__container {
      background-color: #FFFFFF;
      border-bottom: 1px solid #000000;

      .header-redesigned__wrapper {
        .nav-list__desk {
          .nav-list__item {
            .nav-list__link {
              color: #000000;
            }
          }
        }

        .logo {
          a {
            svg {
              fill: #000000;
            }
          }
        }

        .header-redesigned__icons {
          .account-link {
            color: #000000;
          }

          .search-icon, .swym-wishlist, .header-cart__icon {
            svg {
              fill: #000000;
            }
          }
        }
      }
    }
  }

  .header-redesigned__container {
    position: absolute;
    width: 100%;
    transition: all .2s linear;
    border-bottom: 1px solid #FFFFFF;

    .header-redesigned__wrapper {
      position: relative;
      display: flex;
      align-items: center;
      justify-content: space-between;
      max-width: 1440px;
      margin: 0 auto;
      width: 100%;

      padding: 16px 80px;

      .nav-list__desk {
        display: flex;
        width: 35%;

        .nav-list__item {
          margin: 0 45px 0 0;

          &:last-child {
            margin: 0;
          }

          .nav-list__link {
            display: block;
            font-family: 'Montserrat', sans-serif;
            font-style: normal;
            font-weight: 700;
            font-size: 16px;
            line-height: 24px;
            color: #FFFFFF;
            transition: color .2s linear;
          }
        }
      }

      .logo {
        a {
          svg {
            transition: all .2s linear;
          }
        }
      }

      .header-redesigned__icons {
        display: flex;
        justify-content: flex-end;
        align-items: center;
        width: 35%;

        .search-icon {
          display: block;
          margin: 0 25px 0 0;
        }

        .account-link {
          display: block;
          margin: 0 25px 0 0;

          font-family: 'Montserrat', sans-serif;
          font-style: normal;
          font-weight: 700;
          font-size: 16px;
          line-height: 24px;
          text-transform: uppercase;
          color: #FFFFFF;
          transition: color .2s linear;
        }

        .swym-wishlist {
          display: block;
          margin: 7px 25px 0 0;
          padding: 0;
        }

        .search-icon, .swym-wishlist, .header-cart__icon {
          svg {
            transition: all .2s linear;
          }
        }

        .header-cart {
          padding: 0;
        }
      }
    }
  }
}

