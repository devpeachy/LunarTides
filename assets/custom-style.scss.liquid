/* ========================== Marquee announcement bar ========================== */
#marquee-announcement-bar {
  .marquee-announcement-bar {
    max-height: 0;
    overflow: hidden;

    &.download {
      max-height: initial;
    }

    .js-marquee {
      display: flex;
      align-items: center;
      border-right: 0;
      border-left: 0;
      padding: 16px 0;
      white-space: nowrap;
    }
  }

  .announcement-bar__item {
    position: relative;
    font-family: Montserrat, sans-serif;
    font-weight: 700;
    font-size: 14px;
    line-height: 16px;
    margin-right: 16px;
    transition: opacity .2s linear;

    @media (min-width: 1024px) {
      font-size: 16px;
      line-height: 24px;
      margin-right: 55px;
    }

    &:hover {
      opacity: 0.8;
    }
  }
}
