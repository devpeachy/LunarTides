// BUTTON
.btn {
    position: relative;
    display: inline-block;
    width: auto;
    height: auto;
    transform: translateX(8px);

    .link {
        position: relative;
        padding: 12px 20px;
        width: 100%;
        text-align: center;
        background-color: #fff;
        font-size: 16px;
        font-weight: 700;
        color: #000;
        z-index: 10;
        display: inline-block;
        transition: all 0.2s linear;
    }
    
    .link-boxshadow {
        position: absolute;
        top: 8px;
        left: -8px;
        width: 100%;
        height: 100%;
        background-color: #7eeb28;
        z-index: 0;
    }
}
