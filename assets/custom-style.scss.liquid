.section__wrapper {
  .container {
    .shopify-section {
      .faq-redesign {
        font-family: 'Montserrat', sans-serif;

        .faq-container {
          max-width: 1062px;
          width: 100%;
          margin: 0 auto;

          .faq-redesign-title {
            padding-top: 81px;
            font-size: 48px;
            font-weight: 800;
            margin-bottom: 40px;
          }

          .accordion-section-redesign {
            margin-bottom: 142px;

            .accordion-title-wrapper {
              display: flex;
              justify-content: center;
              align-items: center;
              padding: 8px 0;
              margin-bottom: 48px;

              .accordion-img {
                width: 32px;
                height: 32px;
                margin-right: 8px;
              }

              .accordion-title {
                font-size: 24px;
                font-weight: 700;
              }
            }

            .question-content-wrapper {
              border-bottom: 1px solid #000000;

              .accordion-redesign {
                position: relative;
                transition: 0.4s;
                display: flex;
                justify-content: space-between;
                align-items: center;

                .accordion-question {
                  padding: 16px 24px 0;
                  font-size: 16px;
                  font-weight: 700;
                  margin-bottom: 16px;
                  width: 96%;
                  text-transform: uppercase;
                  cursor: pointer;
                }
              }

              .accordion-panel {
                padding: 0 24px 0;
                margin-bottom: 0px;
                background-color: white;
                overflow: hidden;
                max-height: 0;
                transition: max-height 0.2s ease-out;
              }


              .accordion-redesign:before {
                content: '';
                transform: rotate(90deg);
                width: 20px;
                height: 3px;
                position: absolute;
                top: 50%;
                transform: translate(0, -50%);
                background-color: #2E3A59;
                right: 12px;
              }

              .accordion-redesign:after {
                content: '';
                width: 20px;
                height: 3px;
                position: absolute;
                transform: rotate(90deg);
                background-color: #2E3A59;
                right: 12px;
              }

              .active:after {
                content: '';
                width: 0px;
                height: 0px;
                position: absolute;
                transform: rotate(90deg);
                background-color: #2E3A59;
                right: 12px;
              }
            }
          }
        }
      }
    }
  }
}

