.content-with-products {
  padding: 32px 0;

  @media (min-width: 1023px) {
    padding: 66px 0;
  }

  .content-with-products__container {
    width: 100%;
    max-width: 1440px;
    margin: 0 auto;
    padding: 0 16px;

    @media (min-width: 767px) {
      padding: 0 20px;
    }

    @media (min-width: 1023px) {
      padding: 0 40px;
    }

    @media (min-width: 1366px) {
      padding: 0 80px;
    }
  }

  .content-with-products__inner {

    @media (min-width: 1023px) {
      display: flex;
      justify-content: space-between;
      align-items: center;
      flex-direction: row-reverse;

      &.reverse {
        flex-direction: row;

        .content-with-products__content {
          margin: 0;
          margin-right: 50px;
        }
      }
    }

    @media (min-width: 1365px) {
      &.reverse {
        flex-direction: row;

        .content-with-products__content {
          margin-right: 95px;
        }
      }
    }

    .content-with-products__content {

      @media (min-width: 1023px) {
        width: 50%;
        margin-left: 50px;
      }

      @media (min-width: 1365px) {
        width: 40.2%;
        margin-left: 95px;
      }

      .content-title {
        font-family: 'Druk Wide';
        font-style: normal;
        font-weight: 700;
        font-size: 24px;
        line-height: 32px;
        text-align: center;
        text-transform: uppercase;
        color: #FFFFFF;

        @media (min-width: 1023px) {
          text-align: left;
          font-size: 28px;
          line-height: 36px;
        }

        @media (min-width: 1365px) {
          font-size: 32px;
          line-height: 40px;
        }
      }

      .content-text {
        font-family: 'Montserrat';
        font-style: normal;
        font-weight: 400;
        font-size: 16px;
        line-height: 24px;
        text-align: center;
        color: #FFFFFF;
        padding: 16px 0;

        @media (min-width: 1023px) {
          text-align: left;
        }

        @media (min-width: 1365px) {
          padding: 24px 0;
        }
      }

      .content-subtitle {
        font-family: 'Montserrat';
        font-style: normal;
        font-weight: 700;
        font-size: 24px;
        line-height: 32px;
        text-align: center;
        text-transform: uppercase;
        color: #FFFFFF;

        @media (min-width: 1023px) {
          text-align: left;
        }
      }

      .content-btn {
        margin-top: 32px;
        display: flex;
        justify-content: space-between;
        align-items: center;
        width: 100%;
        height: 48px;
        background: #ffffff;
        border-radius: 0px 24px;
        transition: all .3s linear;

        &:hover {
          opacity: .7;
        }

        @media (min-width: 767px) {
          width: fit-content;
          margin: 32px auto 0;
        }

        @media (min-width: 1023px) {
          margin-left: 0;
        }

        @media (min-width: 1365px) {
          margin-top: 40px;
        }

        .btn-text {
          padding: 0 16px;
          font-family: 'Druk Wide';
          font-style: normal;
          font-weight: 500;
          font-size: 16px;
          line-height: 24px;
          color: #000000;

          @media (min-width: 374px) {
            padding: 0 24px;
          }
        }

        .btn-icon {
          height: 100%;
          display: flex;
          align-items: center;
          justify-content: center;
          padding: 0 16px;
          border-left: 1px solid #000;

          svg .arrow {
            fill: #000000;
            width: 24px;
            height: 14px;
          }

          @media (min-width: 374px) {
            padding: 0 27px;
          }

          @media (min-width: 767px) {
            padding: 0 12px;
          }
        }
      }
    }

    .content-with-products__products {
      margin-top: 32px;
      display: grid;
      grid-template-columns: repeat(2, 1fr);
      gap: 16px;

      @media (min-width: 767px) {
        gap: 20px;
      }

      @media (min-width: 1023px) {
        margin: 0;
        width: 65%;
      }

      @media (min-width: 1365px) {
        gap: 32px;
        width: 53%;
      }

      .product-item {

        @media (min-width: 767px) {
          border: 1px solid #ffffff;
          border-radius: 48px;
          overflow: hidden;
        }

        .image-wrapper {
          position: relative;
          border-radius: 24px;
          overflow: hidden;

          @media (min-width: 767px) {
            border-radius: 0;
          }
      
          .quick-shop__buttons {
            width: 90%;
            margin: 0 auto;
            margin-bottom: 12px;
    
            span.button {
              width: 100%;
              background: #FFFFFF;
              font-weight: 700;
              font-size: 12px;
              line-height: 24px;
              text-transform: uppercase;
              color: #000000;
              border-radius: 0;
              padding: 4px;
              transition: all .2s linear;
  
              @media (min-width: 1023px) {
                padding: 12px;
              }
    
              @media (min-width: 1366px) {
                font-size: 16px;
              }
    
              &:hover {
                background: #000000;
                color: #FFFFFF;
              }
            }
          }
      
          .product-image__badges {
      
            .badge-item {
              position: absolute;
              display: flex;
              justify-content: center;
              align-items: center;
              width: 84px;
              height: 42px;
              top: 0;
              left: 50%;
              transform: translateX(-50%);
    
              font-family: 'Montserrat';
              font-style: normal;
              font-weight: 400;
              font-size: 10px;
              line-height: 14px;
              text-align: center;
              letter-spacing: 0.24em;
              text-transform: uppercase;
              color: #fff;
              background: #000;
              border-radius: 0px 0px 200px 200px;
      
              @media (min-width: 1365px) {
                top: -6px;
                width: 100px;
                height: 47px;
                font-size: 12px;
                padding: 0 18px;
                border-radius: 0px 0px 400px 400px;
              }
      
              &.badge-best-seller {
                z-index: 26;
              }
      
              &.badge-new {
                z-index: 25;
              }
            }
          }

          .sticker-holder {
      
            .sticker-holder__content {
      
              .sale-sticker {
                position: absolute;
                display: flex;
                justify-content: center;
                align-items: center;
                width: 84px;
                height: 42px;
                top: 0;
                left: 50%;
                transform: translateX(-50%);
                z-index: 27;
                background: #000;
                overflow: hidden;
                border-radius: 0px 0px 200px 200px;
          
                @media (min-width: 1365px) {
                  top: -6px;
                  width: 100px;
                  height: 47px;
                  font-size: 12px;
                  padding: 0 18px;
                  border-radius: 0px 0px 400px 400px;
                }
    
                font-family: 'Montserrat';
                font-style: normal;
                font-weight: 400;
                font-size: 10px;
                line-height: 14px;
                text-align: center;
                letter-spacing: 0.24em;
                background: #000;
                text-transform: uppercase;
                color: #fff;
              }
            }
          }
      
          .image-container {
    
            a {
              .image-element__wrap {
                height: 235px;

                @media (min-width: 767px) {
                  height: 460px;
                }

                @media (min-width: 1023px) {
                  height: 360px;
                }
    
                @media (min-width: 1365px) {
                  height: 408px;
                }
        
                img {
                  width: 100%;
                  height: 100%;
                  object-fit: cover;
                }
              }
            }
          }
      
          .thumbnail-overlay__container {
            padding: 0;
            justify-content: flex-end;
            background-color: transparent;
      
            .quick-shop__buttons {
              width: 90%;
              margin: 0 auto;
              margin-bottom: 12px;
      
              span.button {
                width: 100%;
                background: #FFFFFF;
                font-weight: 700;
                font-size: 12px;
                line-height: 24px;
                text-transform: uppercase;
                color: #000000;
                border-radius: 0;
                padding: 4px;
                transition: all .2s linear;
    
                @media (min-width: 1023px) {
                  padding: 12px;
                }
      
                @media (min-width: 1366px) {
                  font-size: 16px;
                }
      
                &:hover {
                  background: #000000;
                  color: #FFFFFF;
                }
              }
            }
          }
        }

        .product-info {
          padding-top: 8px;

          @media (min-width: 767px) {
            display: grid;
            border-top: 1px solid #FFFFFF;
            padding: 16px 40px;
          }

          @media (min-width: 1023px) {
            padding: 16px 20px;
          }

          @media (min-width: 1365px) {
            padding: 16px 16px 16px 24px;
          }

          .product-info__title {
            font-family: 'Montserrat';
            font-style: normal;
            font-weight: 400;
            font-size: 16px;
            line-height: 24px;
            text-transform: uppercase;
            color: #FFFFFF;

            @media (min-width: 767px) {
              grid-row-start: 1;
              grid-column-start: 1;
              grid-column-end: 3;
            }

            @media (min-width: 1365px) {
              grid-column-end: 3;
              font-weight: 400;
              line-height: 32px;
            }
          }

          .product-info__btn {
            display: flex;
            align-items: center;

            @media (min-width: 767px) {
              grid-column-start: 1;
              grid-column-end: 2;
              grid-row-start: 2;
            }

            span {
              font-family: 'Montserrat';
              font-style: normal;
              font-weight: 500;
              font-size: 16px;
              line-height: 24px;
              color: #E5B0E0;
              margin-right: 8px;
            }
          }

          .icons-star {
            display: block;
            padding: 8px 0;

            @media (min-width: 767px) {
              text-align: right;
              grid-column-start: 2;
              grid-column-end: 4;
              grid-row-start: 2;
              padding: 0;
            }

            @media (min-width: 1365px) {
              grid-row-start: 1;
            }
          }
        }
      }
    }
  }
}
