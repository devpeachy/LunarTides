        grid-template-columns: repeat(2, 1fr);

        @media (min-width: 767px) {
          width: 50%;
        }

        @media (min-width: 1366px) {
          width: 60%;
        }

        .block-item {

          img {
            width: 100%;
            height: 100%;
            object-fit: cover;
          }
        }

        .block-image {
          grid-column-start: 1;
          grid-column-end: 2;
          border-width: 0px 2px 2px 0px;
          border-style: solid;
          border-color: #000000;

          @media (min-width: 1023px) {
            border-width: 0px 0px 2px 0px;
          }

          @media (min-width: 1366px) {
            grid-column-start: 1;
            grid-column-end: 3;
          }
        }

