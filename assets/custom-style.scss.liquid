            width: 1000px;
            overflow: hidden;
          }
        }

        .js-marquee {
          display: flex;
          align-items: center;
          white-space: nowrap;

          .bar__item {
            font-weight: 700;
            font-size: 24px;
            line-height: 32px;
            letter-spacing: 0.03em;
            text-transform: uppercase;
            transition: opacity .2s linear;
            margin-right: 12px;
            cursor: pointer;

            &:hover {
              opacity: .8;
