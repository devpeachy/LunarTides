      .footer-redesign__social {
        margin-top: 40px;
        display: flex;
        align-items: center;
        justify-content: center;

        @media (min-width: 767px) {
          margin-top: 20px;
        }

        @media (min-width: 1366px) {
          margin-top: 0;
          flex-direction: column;
          position: absolute;
          top: 0;
          right: 0;
        }

        .social-item {
          width: 40px;
          height: 40px;
          margin: 0 20px;

          @media (min-width: 1366px) {
            margin: 0;
            margin-bottom: 30px;

            &:last-child {
