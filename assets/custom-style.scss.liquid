/* ========================== Reviews Banner ========================== */

.reviews-banner-section {
    padding: 8px 0;
    background-position: 0 0;
    background-repeat: no-repeat;
    background-size: cover;
    position: relative; 
    

    .reviews-desktop-mobile {
        display: none;
    }

    .reviews-desktop-background {
        display: block;
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        z-index: -20;
    }

    .reviews-container {
        max-width: 1200px;
        width: 100%;
        margin: 0 auto;

        .review-wrapper {
            display: flex;
            justify-content: space-between !important;
            align-items: center;
            text-align: right;

            .reviews-content-wrapper {
                padding: 20px 40px 20px 81px;
                display: inline-block;
                text-align: left;

                &.content-right {
                    padding: 20px 81px 20px 40px;
                }

                .reviews-title {
                    font-size: 24px;
                    font-weight: 700;
                    line-height: 32px;
                    max-width: 300px;
                    margin-bottom: 16px;
                    text-align: left;
                }

                .reviews-text {
                    font-size: 16px;
                    font-weight: 400;
                    line-height: 24px;
                    max-width: 512px;
                    margin-bottom: 40px;
                    text-align: left;
                }

                .reviews-button {
                    max-width: 210px;
                }
            }
            .reviews-images-wrapper {
                display: flex;
                justify-content: space-between;
                text-align: right;
                width: 100%;
                max-width: 785px;

                &.image-center {
                    justify-content: center;
                }

                .reviews-image-wrapper {
                    display: inline-block;
                    width: 49.5%;

                    .reviews-image {
                        width: 100%;
                        height: 100%;
                        object-fit: cover;
                    }
                }
            }
        }
    }
}

