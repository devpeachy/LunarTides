          grid-column-start: 2;
          grid-column-end: 3;
          border-bottom: 2px solid #000000;

          @media (min-width: 1023px) {
            border-bottom: none;
            border-left: 2px solid #000000;
          }
        }

        .block-review {
          grid-column-start: 1;
          grid-column-end: 3;
          padding: 0 16px;
          border-bottom: 2px solid #000000;

          @media (min-width: 767px) {
            border: none;
          }

          @media (min-width: 1366px) {
            grid-column-start: 1;
            grid-column-end: 2;
            grid-row-start: 2;

            display: flex;
            flex-direction: column;
            justify-content: space-around;
          }

          .review-title {
            margin-top: 16px;
            font-weight: 700;
            font-size: 24px;
            line-height: 32px;
            text-transform: uppercase;
