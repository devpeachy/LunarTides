      display: flex;
      flex-direction: column-reverse;
      margin-top: 29px;

      @media (min-width: 767px) {
        flex-direction: row;
        justify-content: space-between;
      }

      @media (min-width: 1366px) {
        justify-content: flex-start;
      }

      .footer-copyright {

        .copyright-text {
          font-weight: 400;
          font-size: 14px;
          line-height: 16px;
          text-align: center;
          color: #7E7E7E;
        }
      }

      .footer-payment-icons {
        max-width: 220px;
        margin: 0 auto;
        display: flex;
        flex-wrap: wrap;
        align-items: center;
        justify-content: center;
        margin-bottom: 29px;

        @media (min-width: 767px) {
          margin: 0;
          max-width: 100%;
        }

        @media (min-width: 1366px) {
          position: absolute;
          bottom: 0;
          left: 50%;
          transform: translateX(-50%);
        }

        .payment-icon {
          width: 22px;
          height: 14px;
          margin: 9px 6px;

          @media (min-width: 767px) {
            margin: 0 6px;
          }
        }
      }
    }

  }
}
/* ========================== New Footer End ========================== */

/* ========================== About Lunar ========================== */
.about-lunar {
  padding-top: 40px;
  border-radius: 40px 40px 0px 0px;

  @media (min-width: 767px) {
    padding-top: 80px;
    padding-bottom: 80px;
  }

  .about-lunar__container {
    max-width: 1440px;
    width: 100%;
    margin: 0 auto;

    @media (min-width: 1023px) {
      padding: 0 60px;
    }

    @media (min-width: 1366px) {
      padding: 0 80px;
    }
  }

  .about-lunar__content {

    .about-lunar__top-content {
      padding: 0 16px;
      margin-bottom: 40px;

      @media (min-width: 767px) {
        padding: 0 20px;
        margin-bottom: 60px;
      }

      @media (min-width: 1023px) {
        padding: 0;
        margin-bottom: 80px;
      }

      @media (min-width: 1366px) {
        margin-bottom: 112px;
      }

      .about-lunar__top-wrapper {

        @media (min-width: 1023px) {
          display: flex;
          align-items: center;
          justify-content: space-between;
        }
        
        .about-lunar__title {
          font-weight: 800;
          font-size: 24px;
          line-height: 32px;
          text-align: center;
          text-transform: uppercase;
          margin-bottom: 25px;

          @media (min-width: 767px) {
            font-size: 36px;
            line-height: 46px;
            margin-bottom: 30px;
          }

          @media (min-width: 1023px) {
            max-width: 278px;
            margin-right: 40px;
            text-align: left;
            margin-bottom: 0;
          }

          @media (min-width: 1366px) {
            max-width: 340px;
            font-size: 48px;
            line-height: 59px;
          }
        }
  
        .about-lunar__blocks {

          @media (min-width: 767px) {
            display: flex;
            align-items: center;
            justify-content: space-between;
            gap: 20px;
          }

          @media (min-width: 1366px) {
            gap: 40px;
          }

          .about-lunar__item {
            max-width: 240px;
            margin: 0 auto;
            margin-bottom: 41px;

            @media (min-width: 767px) {
              margin-bottom: 0;
            }

            @media (min-width: 1023px) {
              max-width: 228px;
              margin-left: 0;
            }

            &:last-child {
              margin-bottom: 0;
            }

            .item-svg {
              margin: 0 auto;
              width: 40px;
              height: 40px;
              margin-bottom: 25px;

              @media (min-width: 1023px) {
                margin-left: 0;
              }
            }

            .item-text {
              width: fit-content;
              font-weight: 700;
              font-size: 16px;
              line-height: 24px;
              text-align: center;

              @media (min-width: 1023px) {
                text-align: left;
              }
            }
          }
        }
      }

      .about-lunar__gif {
        overflow: hidden;
        border: 2px solid #000000;
        border-radius: 40px;
        margin-top: 24px;
        width: 100%;
        height: 32px;

        @media (min-width: 767px) {
          height: 40px;
          margin-top: 32px;
        }
        
        @media (min-width: 1023px) {
          height: 48px;
          margin-top: 40px;
        }

        img {
          width: 100%;
          height: 100%;
          object-fit: cover;
        }
      }
    }

    .about-lunar__main-content {
      overflow: hidden;

      @media (min-width: 767px) {
        display: flex;
        border: 2px solid #000000;
      }

      .about-lunar__block-image {
        display: flex;
        position: relative;
        border-top: 2px solid #000000;
        border-bottom: 2px solid #000000;

        @media (min-width: 767px) {
          width: 50%;
          border: none;
          border-right: 2px solid #000000;
        }

        @media (min-width: 1366px) {
          width: 40%;
        }

        img {
          width: 100%;
          height: 100%;
          object-fit: cover;
        }

        .about-lunar__btn {
          position: absolute;
          bottom: 32px;
          left: 50%;
          transform: translateX(-50%);

          @media (min-width: 1023px) {
            bottom: 40px;
          }

          .link {
            width: 210px;
            transition: all .2s linear;
          }
        }
      }

      .about-lunar__marquee-bar {
        height: 70px;

        @media (min-width: 767px) {
          width: 64px;
          height: 64px;
          transform: rotate(90deg);
        }

        .marquee-bar__wrapper {
          display: flex;
          align-items: center;
          height: 100%;

          @media (min-width: 767px) {
            width: 1000px;
            overflow: hidden;
          }
        }

        .js-marquee {
          display: flex;
          align-items: center;
          white-space: nowrap;

          .bar__item {
            font-weight: 700;
            font-size: 24px;
            line-height: 32px;
            letter-spacing: 0.03em;
            text-transform: uppercase;
            transition: opacity .2s linear;
            margin-right: 12px;
            cursor: pointer;

            &:hover {
              opacity: .8;
            }

            @media (min-width: 1023px) {
              letter-spacing: 0.21em;
              margin-right: 24px;
            }

            @media (min-width: 1366px) {
              margin-right: 32px;
            }
          }
        }
      }

      .about-lunar__block-images-reviews {
        display: grid;
        grid-template-columns: repeat(2, 1fr);

        @media (min-width: 767px) {
          width: 50%;
        }

        @media (min-width: 1366px) {
          width: 60%;
        }

        .block-item {

          img {
            width: 100%;
            height: 100%;
            object-fit: cover;
          }
        }

        .block-image {
          grid-column-start: 1;
          grid-column-end: 2;
          border-width: 0px 2px 2px 0px;
          border-style: solid;
          border-color: #000000;

          @media (min-width: 1023px) {
            border-width: 0px 0px 2px 0px;
          }

          @media (min-width: 1366px) {
            grid-column-start: 1;
            grid-column-end: 3;
          }
        }

        .block-image2 {
          grid-column-start: 2;
          grid-column-end: 3;
          border-bottom: 2px solid #000000;

          @media (min-width: 1023px) {
            border-bottom: none;
            border-left: 2px solid #000000;
          }
        }

        .block-review {
          grid-column-start: 1;
          grid-column-end: 3;
          padding: 0 16px;
          border-bottom: 2px solid #000000;

          @media (min-width: 767px) {
            border: none;
          }

          @media (min-width: 1366px) {
            grid-column-start: 1;
            grid-column-end: 2;
            grid-row-start: 2;

            display: flex;
            flex-direction: column;
            justify-content: space-around;
          }

          .review-title {
            margin-top: 16px;
            font-weight: 700;
            font-size: 24px;
            line-height: 32px;
            text-transform: uppercase;
            color: #000000;

            @media (min-width: 1366px) {
              margin-top: 24px;
            }
          }

          .review-text {
            font-weight: 400;
            font-size: 16px;
            line-height: 24px;
            color: #000000;
            padding: 24px 0;

            @media (min-width: 1366px) {
              padding: 8px 0;
            }
          }

          .review-initials {
            margin-bottom: 16px;
            width: fit-content;
            font-weight: 700;
            font-size: 16px;
            line-height: 24px;
            text-transform: uppercase;
            color: #000000;
          }
        }
      }
    }
  }
}

